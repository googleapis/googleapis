+bazel_dep(name = "googleapis-rules-registry", version = "1.0.0")
+
+rules_registry = use_extension("@googleapis-rules-registry//private/extensions:rules_registry.bzl", "rules_registry")
+use_repo(
+    rules_registry,
+    "com_google_googleapis_imports",
+    "io_bazel_rules_go",
+)