{
  "methodConfig": [{
    "name": [{ "service": "google.storage.v2.Storage"}],
    "timeout": "60s",
    "retryPolicy": {
      "maxAttempts": 5,
      "initialBackoff": "1s",
      "maxBackoff": "60s",
      "backoffMultiplier": 2,
      "retryableStatusCodes": [
        "DEADLINE_EXCEEDED", "UNAVAILABLE"
      ]
    }
  }]
}
