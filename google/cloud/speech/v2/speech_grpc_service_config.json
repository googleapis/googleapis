{
  "methodConfig": [
    {
      "name": [
        {
          "service": "google.cloud.speech.v2.Speech"
        }
      ],
      "timeout": "5000s",
      "retryPolicy": {
        "initialBackoff": "0.100s",
        "maxBackoff": "60s",
        "backoffMultiplier": 1.3,
        "retryableStatusCodes": ["DEADLINE_EXCEEDED", "UNAVAILABLE"]
      }
    }
  ]
}
